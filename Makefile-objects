$(OBJECTDIR)/smooth.h.gch: $(INCLUDEDIR)/smooth.h
	$(ECHO) -n Compiling $(INCLUDEDIR)/smooth.h...
	$(COPIER) $(COPIER_OPTS) $(INCLUDEDIR)/smooth.h $(OBJECTDIR)/smooth.h
	$(COMPILER) $(COMPILER_OPTS) -c $(INCLUDEDIR)/smooth.h -o $(OBJECTDIR)/smooth.h.gch
	$(REMOVER) $(REMOVER_OPTS) $(OBJECTDIR)/smooth.h
	$(ECHO) done.

